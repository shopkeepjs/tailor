{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"outDir": ".svelte-kit/output",
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"types": ["svelte", "node"]
	},
	"include": [
		".svelte-kit/ambient.d.ts",
		".svelte-kit/types/**/$types.d.ts",
		"vite.config.ts",
		"src/**/*.js",
		"src/**/*.ts",
		"src/**/*.svelte",
		"src/**/*.stories.ts",
		".eslintrc.cjs",
		"svelte.config.js",
		"vitest.config.ts",
		"src/**/*.d.ts"
	]
}
